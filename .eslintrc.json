{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-typescript/base",
        "plugin:@typescript-eslint/recommended",
        "prettier/@typescript-eslint",
        "plugin:prettier/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module",
        "project": [
            "./tsconfig.json"
        ]
    },
    "rules": {
        // "no-underscore-dangle": 0,
        // "no-param-reassign": 0,
        // "consistent-return": 0,
        // "no-use-before-define": 0,
        // "no-console": 0
    }
}